{
  "name": "buflymaker",
  "version": "0.1.8",
  "description": "A Visualization Form Design Library(base naiveui) for Vue3.js.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eicesoft/buflymaker/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "module": "./dist/build.es.ts",
  "main": "./dist/build.umd.ts",
  "exports": {
    ".": {
      "import": "./dist/build.es.ts",
      "require": "./dist/build.umd.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lib": "vite build --config vite-lib.config.ts && tsc --project tsconfig.lib.json",
    "preview": "vite preview"
  },
  "keywords": [
    "naiveui",
    "vue3",
    "components",
    "form builder"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eicesoft/buflymaker.git"
  },
  "dependencies": {
    "@vicons/antd": "^0.10.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.24.5",
    "sass": "^1.32.8",
    "vue": "^3.2.25",
    "vue-clipboard3": "^1.0.1",
    "vuedraggable": "^4.1.0"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.2",
    "@vitejs/plugin-vue": "^2.0.0",
    "fast-glob": "^3.2.11",
    "string-hash": "^1.1.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.29.8"
  }
}
